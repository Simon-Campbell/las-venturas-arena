#define MAX_ADMIN_VEHICLES  12
#define MAX_PIZZA_VEHICLES  3
#define MAX_TAXI_VEHICLES    10

enum _:E_TAXI_STATUS
{
	TAXI_STATUS_NONE,
	TAXI_STATUS_DRIVER,
	TAXI_STATUS_PASSENGER
}

new	aVehicle	[ MAX_ADMIN_VEHICLES ];

new
	pizzaVehicle[ MAX_PIZZA_VEHICLES ],
	taxiVehicle [ MAX_TAXI_VEHICLES ];

stock IsPlayerOnPizzaBoy( playerid )
{
	new pVehicle = GetPlayerVehicleID( playerid );
	
	if ( !pVehicle ) return 0;
	
	for ( new i = 0; i < MAX_PIZZA_VEHICLES; i++ )
	{
	    if ( pVehicle == pizzaVehicle[ i ] )
	        return 1;
	}
	
	return 0;
}

stock IsPlayerInTaxi( playerid )
{
	new pVehicle = GetPlayerVehicleID( playerid );
	
	if ( !pVehicle ) return _:TAXI_STATUS_NONE;
	
	for ( new i = 0; i < MAX_TAXI_VEHICLES; i++ )
	{
	    if ( pVehicle == taxiVehicle[ i ] )
	    {
	        new pState = GetPlayerState( playerid );
	        
	        if ( pState == PLAYER_STATE_DRIVER )
	        	return _:TAXI_STATUS_DRIVER;
			else if ( pState == PLAYER_STATE_PASSENGER )
			    return _:TAXI_STATUS_PASSENGER;
			else
			    return _:TAXI_STATUS_NONE;
		}
	}
	return _:TAXI_STATUS_NONE;
}

stock PizzaBoyHandler( playerid )
{
	if      ( pData[ playerid ][ P_CHECKPOINT_AREA ] != CP_PIZZA_HOME || pData[ playerid ][ P_CHECKPOINT_AREA ] != CP_PIZZA_DELIVER )
		return 0;
		
	else if	( !IsPlayerOnPizzaBoy( playerid )
		return GameTextForPlayer( playerid, "You must be on a PizzaBoy moped.", 5000, 1 );
		
	else if ( pData[ playerid ][ P_ACTIVITY ] == _:P_ACTIVITY_PIZZA_DELIVER )
	{
	    // Taking the delivery to destination.
	    
	    new pString[ 256 ];
	    
	    format( pString, sizeof( pString ), "Pizza Delivered~n~~w~$%d", pData[ playerid ][ P_PIZZA_PAYMENT ] );
	    GameTextForPlayer( playerid, pString, 5000, 1 );
	    AC_GivePlayerMoney( playerid, pData[ playerid ][ P_PIZZA_PAYMENT ] );
	    
	    return 1;
	}
	else if ( pData[ playerid ][ P_ACTIVITY ] == _:P_ACTIVITY_PIZZA_RETURN )
	{
	    // Getting a new delivery order.
	}
	
	else    return 0;
}

